{
  "compilerOptions": {
    "lib": [
      "es6",
      "dom",
      "es2016"
    ],
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@assets/*": [
        "src/assets/*"
      ],
      "@styles/*": [
        "src/styles/*"
      ],
      "@utils/*": [
        "src/utils/*"
      ],
      "@store/*": [
        "src/store/*"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "esModuleInterop": true
  }
}
